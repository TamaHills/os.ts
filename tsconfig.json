{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "jsx": "react",
    "jsxFactory": "h",
    "esModuleInterop": true,
    "sourceMap": true,
    "allowJs": true,
    "target": "ES5",
    "lib": [
      "es6",
      "dom"
    ],
    "rootDir": "src",
    "moduleResolution": "node",
    "baseUrl": "src/",
    "paths": {
      "util/*": [
        "util/*"
      ],
      "os/*": [
        "os/*"
      ],
      "apps/*": [
        "apps/*"
      ]
    },
    "types": [
      "./types"
    ]
  }
}