{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "jsx": "react",
    "jsxFactory": "h",
    "esModuleInterop": true,
    "sourceMap": true,
    "allowJs": true,
    "target": "ES5",
    "lib": [
      "es6",
      "dom"
    ],
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "util/*": [
        "src/util/*"
      ],
      "os/*": [
        "src/os/*"
      ],
      "apps/*": [
        "src/apps/*"
      ]
    },
    "types": [
      "./node_modules/@types/node",
      "./types"
    ]
  }
}